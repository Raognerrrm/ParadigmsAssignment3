FUNCTION main(n)
VARS cond;
BEGIN
  cond = (n == 0);
  IF cond THEN
  BEGIN
    cond = 1;
    RETURN cond;
  END
  ELSE
  BEGIN
    cond = (n == 1);
    IF cond THEN
    BEGIN
      RETURN n;
    END
    ELSE
    BEGIN
      cond = (n == 2);
      IF cond THEN
      BEGIN
        RETURN n;
      END;
      cond = (n == 3);
      IF cond THEN
      BEGIN
        cond = 6;
        RETURN n;
      END;
    END;
  END;
  n = 3;
  RETURN n;
END